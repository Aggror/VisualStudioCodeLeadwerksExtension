{
    "name": "Leadwerks",
    "displayName": "Leadwerks",
    "description": "Leadwerks API and Lua extension",
    "version": "0.6.4",
    "publisher": "Jorn Theunissen - \"Aggror\"",
    "homepage": "https://github.com/Aggror/VisualStudioCodeLeadwerksExtension",
    "engines": {
        "vscode": "^1.21.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Aggror/VisualStudioCodeLeadwerksExtension.git"
    },
    "categories": [
        "Snippets",
        "Languages",
        "Linters"
    ],
    "keywords": [
        "leadwerks",
        "game engine",
        "lua"
    ],
    "activationEvents": [],
    "main": "./out/src/extension",
    "contributes": {
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "lua",
                "path": "./snippets/leadwerksapisnippets.json"
            }
        ],
        "keybindings": [
            {
                "command": "extension.sidePreview",
                "key": "ctrl+shift+h",
                "mac": "ctrl+shift+h",
                "when": "editorTextFocus"
            }
        ]
    },
    "icon": "images/icon.png",
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "extensionDependencies": [
        "trixnz.vscode-lua"
    ]
}