{
    "name": "Leadwerks",
    "displayName": "Leadwerks",
    "description": "Leadwerks API and Lua extension",
    "version": "0.2.1",
    "publisher": "aggror",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Snippets",
        "Game engine",
    ],
    "keywords": [
        "leadwerks",
        "game engine",
        "lua"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/snippets.json"
            }
        ],
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "icon": "images/icon.png",
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "extensionDependencies": [
        "trixnz.vscode-lua"
    ]
}